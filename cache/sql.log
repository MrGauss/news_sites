INSERT INTO posts ("title", "alt_title", "descr", "short_post", "full_post", "comment", "svector", "keywords", "posted", "fixed", "static", "category", "author_id") VALUES ('Черкаські рятувальники ліквідували три пожежі спричинені необережним поводженням з вогнем', 'fcebd0e7894444cfc6ad497a8cc7178f', 'Черкаські рятувальники ліквідували три пожежі спричинені необережним поводженням з вогнем', '30 жовтня о 15:45 на території приватного домоволодіння на вулиці Польовій у селі Червона Слобода Черкаського району, виникла пожежа у надвірній споруді. Повідомили у ДСНС Черкащини. На боротьбу з вогнем оперативно прибули рятувальники 1-ї державної пожежно-рятувальної частини міста Черкаси та місцева пожежна команда із села Леськи. Спільними зусиллями вогнеборців пожежу о 16:11 вдалося локалізувати, а о 17:03 ліквідувати. До гасіння залучалися 2 одиниці спецтехніки та 5 чоловік особового складу. Вогнем пожежі знищено 2 м.куб. дров, що зберігалися в середині споруди. За попередніми даними, причиною пожежі стало необережне поводження з вогнем. З аналогічної причини, вночі 31 жовтня двічі горіло сміття в місті Черкаси. Завдяки професійним діям рятувальників 3-ї та 2-ї пожежно-рятувальних частин пожежі вдалося успішно ліквідувати та запобігти подальшому поширенню вогню.', '<p class=\"bb_p\">30 жовтня о 15:45 на території приватного домоволодіння на вулиці Польовій у селі Червона Слобода Черкаського району, виникла пожежа у надвірній споруді. Повідомили у ДСНС Черкащини.</p>
<p class=\"bb_p align_center\"><img class="post_img" src="/uploads/images/2019-10-31/2019-10-31_20-38-09_28.png" alt="" title="" /></p>
<p class=\"bb_p\">На боротьбу з вогнем оперативно прибули рятувальники 1-ї державної пожежно-рятувальної частини міста Черкаси та місцева пожежна команда із села Леськи. Спільними зусиллями вогнеборців пожежу о 16:11 вдалося локалізувати, а о 17:03 ліквідувати. До гасіння залучалися 2 одиниці спецтехніки та 5 чоловік особового складу.</p>
<p class=\"bb_p\">Вогнем пожежі знищено 2 м.куб. дров, що зберігалися в середині споруди. За попередніми даними, причиною пожежі стало необережне поводження з вогнем.</p>
<p class=\"bb_p\">З аналогічної причини, вночі 31 жовтня двічі горіло сміття в місті Черкаси. Завдяки професійним діям рятувальників 3-ї та 2-ї пожежно-рятувальних частин пожежі вдалося успішно ліквідувати та запобігти подальшому поширенню вогню.</p><div class="source"><p class=\"bb_p align_right\"><a rel="nofollow noreferrer" href="https://dzvin.media/news/cherkaski-ryatuvalnyky-likviduvaly-try-pozhezhi-sprychyneni-neoberezhnym-povodzhennyam-z-vognem/" target="_blank">За матеріалами "dzvin.media"</a></p></div>', 'https://dzvin.media/news/cherkaski-ryatuvalnyky-likviduvaly-try-pozhezhi-sprychyneni-neoberezhnym-povodzhennyam-z-vognem/', '', '', '1', '0', '0', '1', '1') RETURNING id;
